  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "npm-run-all -p serve generate-live-editing",
    "start:styles": "npm-run-all -p serve \"generate-live-editing -- --styles {1}\" --",
    "start:watch-live-editing": "npm-run-all -p serve watch-live-editing",
    "start:styles:watch-live-editing": "npm-run-all -p serve \"watch-live-editing -- --styles {1}\" --",
    "generate-live-editing": "gulp generate-live-editing",
    "watch-live-editing": "gulp watch-live-editing",
    "build": "gulp generate-live-editing && node --max_old_space_size=2048 node_modules/@angular/cli/bin/ng build --prod --aot=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/IgniteUI/igniteui-angular-samples/repository.git"
  },
